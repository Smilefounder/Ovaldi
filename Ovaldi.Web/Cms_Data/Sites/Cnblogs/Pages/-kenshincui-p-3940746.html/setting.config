&lt;/span&gt;&lt;span style="background: white; color: black"&gt;@&lt;/span&gt;&lt;span style="background: white; color: blue"&gt;interface &lt;/span&gt;&lt;span style="background: white; color: black"&gt;KCQQContactViewController ()

