&lt;p&gt;页面效果：&lt;/p&gt;
&lt;p&gt;&lt;a href="http://images.cnitblog.com/blog/639859/201408/291414465482657.jpg"&gt;&lt;img src="http://images.cnitblog.com/blog/639859/201408/291415485634509.jpg" alt=""&gt;&lt;/a&gt;&lt;/p&gt;
&lt;p&gt;jQuery代码（第一次）：&lt;/p&gt;
&lt;pre class="csharpcode"&gt;        &amp;lt;script type=&lt;span class="str"&gt;"text/javascript"&lt;/span&gt;&amp;gt;
            $(&lt;span class="kwrd"&gt;function&lt;/span&gt;(){
                &lt;span class="rem"&gt;//添加tr背景色&lt;/span&gt;
                $(&lt;span class="str"&gt;"table tr:first"&lt;/span&gt;).css(&lt;span class="str"&gt;"background-color"&lt;/span&gt;,&lt;span class="str"&gt;"#696969"&lt;/span&gt;);
                $(&lt;span class="str"&gt;"table tr:even"&lt;/span&gt;).not(&lt;span class="str"&gt;"table tr:first"&lt;/span&gt;).css(&lt;span class="str"&gt;"background-color"&lt;/span&gt;,&lt;span class="str"&gt;"#98F5FF"&lt;/span&gt;);
                $(&lt;span class="str"&gt;"table tr:odd"&lt;/span&gt;).css(&lt;span class="str"&gt;"background-color"&lt;/span&gt;,&lt;span class="str"&gt;"#90EE90"&lt;/span&gt;);
                &lt;span class="rem"&gt;//删除按钮点击事件&lt;/span&gt;
                $(&lt;span class="str"&gt;"input[type='button']"&lt;/span&gt;).click(&lt;span class="kwrd"&gt;function&lt;/span&gt;(){
                    $(&lt;span class="kwrd"&gt;this&lt;/span&gt;).parent().parent().line();
                });
                    
            })
    
            $.fn.line = &lt;span class="kwrd"&gt;function&lt;/span&gt; () {
                &lt;span class="kwrd"&gt;return&lt;/span&gt; $(&lt;span class="kwrd"&gt;this&lt;/span&gt;).children(&lt;span class="str"&gt;"td"&lt;/span&gt;).each(&lt;span class="kwrd"&gt;function&lt;/span&gt; (index) {
                    $(&lt;span class="kwrd"&gt;this&lt;/span&gt;).children().attr(&lt;span class="str"&gt;"disabled"&lt;/span&gt;, &lt;span class="str"&gt;"disabled"&lt;/span&gt;).children().attr(&lt;span class="str"&gt;"disabled"&lt;/span&gt;, &lt;span class="str"&gt;"disabled"&lt;/span&gt;);
                    &lt;span class="kwrd"&gt;if&lt;/span&gt; (index == 0) {&lt;span class="rem"&gt;//重点部分&lt;/span&gt;
                        &lt;span class="kwrd"&gt;var&lt;/span&gt; t = $(&lt;span class="kwrd"&gt;this&lt;/span&gt;).offset().top + $(&lt;span class="kwrd"&gt;this&lt;/span&gt;).height();&lt;span class="rem"&gt;//1、获得对应行，第一列相对于浏览器顶部的位移&lt;/span&gt;
                        &lt;span class="kwrd"&gt;var&lt;/span&gt; l = $(&lt;span class="kwrd"&gt;this&lt;/span&gt;).offset().left;&lt;span class="rem"&gt;//2、获得对应行，第一列相对于浏览器左侧的位移&lt;/span&gt;
                        &lt;span class="kwrd"&gt;var&lt;/span&gt; w = $(&lt;span class="kwrd"&gt;this&lt;/span&gt;).parent(&lt;span class="str"&gt;"tr"&lt;/span&gt;).width();&lt;span class="rem"&gt;//3、获得对应行的宽度&lt;/span&gt;
                        $(&lt;span class="kwrd"&gt;this&lt;/span&gt;).children(&lt;span class="str"&gt;"*:last"&lt;/span&gt;).after(&lt;span class="str"&gt;"&amp;lt;div style='outline:#000 solid 1px; position:absolute; left:"&lt;/span&gt; + l + &lt;span class="str"&gt;"px;top:"&lt;/span&gt; + t + &lt;span class="str"&gt;"px;width:"&lt;/span&gt; + w + &lt;span class="str"&gt;"px;'&amp;gt;&amp;lt;/div&amp;gt;"&lt;/span&gt;);&lt;span class="rem"&gt;//4&lt;/span&gt;
                    }
                });
            }
        &amp;lt;/script&amp;gt;&lt;/pre&gt;
