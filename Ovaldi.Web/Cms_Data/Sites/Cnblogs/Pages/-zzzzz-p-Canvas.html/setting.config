&lt;span style="color: #008080;"&gt; 11&lt;/span&gt; &lt;span style="background-color: #f5f5f5; color: #ff0000;"&gt;            padding&lt;/span&gt;&lt;span style="background-color: #f5f5f5; color: #000000;"&gt;:&lt;/span&gt;&lt;span style="background-color: #f5f5f5; color: #0000ff;"&gt; 0&lt;/span&gt;&lt;span style="background-color: #f5f5f5; color: #000000;"&gt;;&lt;/span&gt;
